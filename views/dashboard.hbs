<div class="content px-3">
        <ul class="nav nav-underline menu-2">
          <li class="nav-item">
            <a
              class="nav-link active border-danger"
              aria-current="page"
              href="/"
              >Dashboard</a
            >
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Team Dashboard</a>
          </li>
        </ul>
        <div class="container mx-auto">
          <div class="row mx-auto">
            <div class="col-lg-2 col-md-4 col-sm-6 mb-3">
              <div class="card text-center" style="width: 10rem; height: 9rem">
                <div class="card-body">
                  <div class="circle mx-auto" style="background: #effff0">
                    <img
                      src="/images/test_cases.svg"
                      alt=""
                      class="centered-image"
                    />
                  </div>
                  <h6 class="card-title mt-3 mb-0">Test Cases</h6>
                  <p class="card-text mb-0">
                    <span class="text-danger m-0 p-0">{{countTestCase}}</span> / 100
                  </p>
                </div>
              </div>
            </div>

            <div class="col-lg-2 col-md-4 col-sm-6 mb-3">
              <div class="card text-center" style="width: 10rem; height: 9rem">
                <div class="card-body">
                  <div class="circle mx-auto" style="background: #f2f8ff">
                    <img
                      src="/images/requirements.svg"
                      alt=""
                      class="centered-image"
                    />
                  </div>
                  <h6 class="card-title mt-3 mb-0">All Projects</h6>
                  <p class="card-text mb-0">
                    <span class="text-danger m-0 p-0">{{countProject}}</span> / 100
                  </p>
                </div>
              </div>
            </div>

            <div class="col-lg-2 col-md-4 col-sm-6 mb-3">
              <div class="card text-center" style="width: 10rem; height: 9rem">
                <div class="card-body">
                  <div class="circle mx-auto" style="background: #fef8f5">
                    <img
                      src="/images/test_runs.svg"
                      alt=""
                      class="centered-image"
                    />
                  </div>
                  <h6 class="card-title mt-3 mb-0">Test Runs</h6>
                  <p class="card-text mb-0">
                    <span class="text-danger m-0 p-0">{{countTestRun}}</span> / 100
                  </p>
                </div>
              </div>
            </div>

            <div class="col-lg-2 col-md-4 col-sm-6 mb-3">
              <div class="card text-center" style="width: 10rem; height: 9rem">
                <div class="card-body">
                  <div class="circle mx-auto" style="background: #f6f1ff">
                    <img
                      src="/images/issues.svg"
                      alt=""
                      class="centered-image"
                    />
                  </div>
                  <h6 class="card-title mt-3 mb-0">Issues</h6>
                  <p class="card-text mb-0">
                    <span class="text-danger m-0 p-0">{{countIssue}}</span> / 100
                  </p>
                </div>
              </div>
            </div>

            <div class="col-lg-2 col-md-4 col-sm-6 mb-3">
              <div class="card text-center" style="width: 10rem; height: 9rem">
                <div class="card-body">
                  <div class="circle mx-auto" style="background: #fff8ed">
                    <img
                      src="/images/storage.png"
                      alt=""
                      class="centered-image"
                      style="max-width: 60%; max-height: 60%"
                    />
                  </div>
                  <h6 class="card-title mt-3 mb-0">Storage</h6>
                  <p class="card-text mb-0">
                    <span class="text-danger m-0 p-0">{{totalSize}}</span> / 10MB
                  </p>
                </div>
              </div>
            </div>

            <div class="col-lg-2 col-md-4 col-sm-6 mb-3">
              <div class="card text-center" style="width: 10rem; height: 9rem">
                <div class="card-body">
                  <div class="circle mx-auto" style="background: #f2f8ff">
                    <img
                      src="/images/users.png"
                      alt=""
                      class="centered-image"
                      style="max-width: 60%; max-height: 60%"
                    />
                  </div>
                  <h6 class="card-title mt-3 mb-0">Users</h6>
                  <p class="card-text mb-0">
                    <span class="text-danger m-0 p-0">{{countUser}}</span> / 100
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="container my-2">
          <div class="row p-3 bg-white rounded">
            
            
            <div class="col">
              <form id="projectForm" action="/" method="get">
                <select
                  class="form-select"
                  style="background-color: #f5f5f5"
                  aria-label="Default select example"
                  name="projectId"
                  onchange="document.getElementById('projectForm').submit()"
                >
                  {{#if projectName}}
                  <option value="">{{projectName}}</option>
                  {{/if}}
                  
                  <option value="0">All Ongoing Projects</option>
                  {{#each projects}}
                  <option value="{{_id}}">{{Name}}</option>
                  {{/each}}
                </select>
              </form>
            </div>
            

           

            <div class="col d-flex align-middle">
              <img
                src="/images/test_cases.svg"
                alt=""
                style="width: 1.5rem; height: 1.5rem; margin: auto 0"
              />
              <p class="m-2"><span class="text-danger">{{countTestCaseProject}}</span> Test Cases</p>
            </div>

            <div class="col d-flex align-middle">
              <img
                src="/images//test_runs.svg"
                alt=""
                style="width: 1.5rem; height: 1.5rem; margin: auto 0"
              />
              <p class="m-2"><span class="text-danger">{{countTestRunProject}}</span> Test Runs</p>
            </div>

            <div class="col d-flex align-middle">
              <img
                src="/images/issues.svg"
                alt=""
                style="width: 1.5rem; height: 1.5rem; margin: auto 0"
              />
              <p class="m-2"><span class="text-danger">{{countIssueProject}}</span> Issues</p>
            </div>

            <div class="col d-flex align-middle">
              <img
                src="/images/requirements.svg"
                alt=""
                style="width: 1.5rem; height: 1.5rem; margin: auto 0"
              />
              <p class="m-2"><span class="text-danger">{{countReportProject}}</span> Reports</p>
            </div>
          </div>
        </div>

        <div class="container px-0">
          <div class="card bg-white rounded my-2">
            <div class="card-header d-flex justify-content-between">
              <h6 class="mb-0 py-2">Overall Activities</h6>
              <div class="">
                <button
                  type="button"
                  class="btn btn-danger btn-graph"
                  onclick="switchDisplay('graph')"
                >
                  Graph
                </button>
                <button
                  type="button"
                  class="btn btn-outline-danger btn-list"
                  onclick="switchDisplay('list')"
                >
                  List
                </button>
              </div>
            </div>
            <div class="card-body">
              <canvas
                class="w-100 h-100"
                id="myChart"
                style="display: block"
                data-labels='{{{labels}}}' 
                data-data='{{{data}}}'
              ></canvas>
              <div id="listContainer" style="display: none">
                {{#each activities}}
                <div class="activity d-flex border-bottom border-1 d-flex justify-content-between py-2">
                  <div class="d-flex align-items-center">
                    <img src="{{UserImg}}" alt="User Image" class="rounded-circle mx-2" style="width: 2rem; height: 2rem">
                    <span class="user-name fw-bold">{{UserName}}</span>
                    <span class="active">
                      {{Action}} - {{Title}}
                    </span
                    >
                  </div>
                  <div class="d-flex align-items-center">
                    <span class="day">{{formattedDate}}</span>
                  </div>
                </div>
                {{/each}}
              </div>
            </div>
          </div>

          <script src="/js/dashboard-chart.js"></script>
        </div>
      </div>